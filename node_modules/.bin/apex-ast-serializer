#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/sh" ]; then
  exec "$basedir/sh"  "$basedir/../prettier-plugin-apex/vendor/apex-ast-serializer/bin/apex-ast-serializer" "$@"
else 
  exec sh  "$basedir/../prettier-plugin-apex/vendor/apex-ast-serializer/bin/apex-ast-serializer" "$@"
fi
